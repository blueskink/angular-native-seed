{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "nativescript/app/**/*.js": true,
    "build/**/*.app/**": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "node_modules/**": true,
    "build/**": true,
    "nativescript/node_modules/**": true,
    "nativescript/platforms/**": true,
    "nativescript/hooks/**": true,
    "nativescript/app/App_Resources/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "build": true,
    "nativescript/node_modules": true,
    "nativescript/platforms": true,
    "nativescript/hooks": true,
    "nativescript/App_Resources": true
  },
  "tslint.exclude": "build/**/*.app/**"
}
