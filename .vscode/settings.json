{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "src/app/**/*.js": true,
    "src/environment/**/*.js": true,
    "nativescript/app/**/*.js": true,
    "nativescript/app/**/*.css": true,
    "nativescript/app/app": true,
    "build/**/*.app": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "build/**": true,
    "nativescript/platforms/**": true,
    "nativescript/hooks/**": true,
    "nativescript/app/App_Resources/**": true,
    "nativescript/app/app/**": true,
    "nativescript/app/**/*.js": true,
    "nativescript/app/enviroments/**": true,
    "nativescript/app/assets/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "build": true,
    "nativescript/platforms": true,
    "nativescript/hooks": true,
    "nativescript/App_Resources": true,
    "nativescript/app/app": true,
    "nativescript/app/**/*.js": true,
    "nativescript/app/enviroments": true,
    "nativescript/app/assets": true
  },
  "tslint.exclude": [
    "**/dist/**",
    "**/build/**",
    "**/node_modules/**",
    "build/**/*.app/**",
    "nativescript/app/app/**"
  ]
}
