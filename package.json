{
  "name": "angular-native-seed",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "main": "main.js",
  "files": [

  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:scss": "stylelint --syntax scss \"src/scss/**/*.scss\"",
    "lint:all": "npm run lint && npm run lint:scss",
    "e2e": "ng e2e",
    "prestart:ios": "npm run symlink",
    "start:ios": "cd nativescript && tns run ios",
    "prestart:android": "npm run symlink",
    "start:android": "cd nativescript && tns run android",
    "coverage": "coveralls < coverage/lcov.info",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app ssr --output-hashing false",
    "webpack:server": "webpack --config webpack.server.js --progress --colors",
    "prestart:desktop": "npm run build",
    "start:desktop": "electron .",
    "prebuild:desktop:win": "npm run build",
    "build:desktop:win": "electron-packager . --platform win32 --out ./build",
    "prebuild:desktop:mac": "npm run build",
    "build:desktop:mac": "electron-packager . --platform darwin --out ./build",
    "symlink": "node symlink.js",
    "release": "standard-version"
  },
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/platform-server": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@angular/service-worker": "^5.2.0",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "rxjs": "^5.5.2",
    "ts-loader": "^3.2.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "electron": "^1.7.10",
    "electron-packager": "^10.1.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "0.0.32",
    "protractor": "~5.1.2",
    "standard-version": "^4.3.0",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^2.2.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "last 2 versions"
  ]
}
